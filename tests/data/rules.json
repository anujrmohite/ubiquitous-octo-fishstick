{
    "total_amount": "quantity * unit_price",
    "profit_margin": "unit_price - cost_price",
    "profit": "quantity * (unit_price - cost_price)",
    "shipping_cost": "weight_kg * 2.5 + 5",
    "discount": "max(0, total_amount * 0.1 if total_amount > 500 else 0)",
    "final_amount": "total_amount - discount + shipping_cost",
    "stock_after_order": "stock_level - quantity",
    "reorder_needed": "1 if stock_after_order < 50 else 0",
    "customer_region": "region",
    "product_category": "category"
}